<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:simpleType name="clave1">
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z0-9]*"/>
            <xs:length value="10"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="clave2">
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z0-9]{10}"/>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>